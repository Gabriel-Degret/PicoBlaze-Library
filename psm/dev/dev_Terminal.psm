; Constants
CONSTANT DEV_TERM_TEXTCOLOR_BLACK,		30		; Black
CONSTANT DEV_TERM_TEXTCOLOR_RED,			31		; Red
CONSTANT DEV_TERM_TEXTCOLOR_GREEN,		32		; Green
